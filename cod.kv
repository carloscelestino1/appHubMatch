            MDLabel:
                font_style: "Caption"
                text: "Li e concordo com os termos"
                halign:'center'
                pos_hint: {'center_x': .4, 'center_y': .2}
                text_color: "white"
                user_font_size: "10sp"
            MDFillRoundFlatButton:
                md_bg_color: "1F4935"
                opacity: 1
                text: "Cadastrar"
                text_color: "black"
                font_size:14
                text_color:1,1,1,1
                pos_hint: {'center_x': .8, 'center_y': .1}
                
                on_press: root.register_startup()

